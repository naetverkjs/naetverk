{
  "version": 1,
  "projects": {
    "angular": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "samples/angular",
      "sourceRoot": "samples/angular/src",
      "prefix": "naetverkjs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/samples/angular",
            "index": "samples/angular/src/index.html",
            "main": "samples/angular/src/main.ts",
            "polyfills": "samples/angular/src/polyfills.ts",
            "tsConfig": "samples/angular/tsconfig.app.json",
            "aot": true,
            "assets": [
              "samples/angular/src/favicon.ico",
              "samples/angular/src/assets"
            ],
            "styles": [
              "samples/angular/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "samples/angular/src/environments/environment.ts",
                  "with": "samples/angular/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "samples/angular/tsconfig.app.json",
              "samples/angular/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!samples/angular/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "samples/angular/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/samples/angular"
          ]
        }
      }
    },
