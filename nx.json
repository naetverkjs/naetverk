{
  "npmScope": "naetverkjs",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "naetverk": {
      "tags": ["library"]
    },
    "angular": {
      "tags": ["sample"]
    },
    "angular-e2e": {
      "tags": ["sample"],
      "implicitDependencies": ["angular"]
    },
    "react": {
      "tags": ["sample"]
    },
    "react-e2e": {
      "tags": ["sample"],
      "implicitDependencies": ["react"]
    },
    "angular-renderer": {
      "tags": ["plugin"]
    },
    "connection-plugin": {
      "tags": ["plugin"]
    },
    "area-plugin": {
      "tags": ["plugin"]
    },
    "lifecycle-plugin": {
      "tags": ["plugin"]
    },
    "keyboard-plugin": {
      "tags": ["plugin"]
    },
    "arrange-plugin": {
      "tags": ["plugin"]
    },
    "history-plugin": {
      "tags": ["plugin"]
    },
    "react-renderer": {
      "tags": ["plugin"]
    },
    "selection-plugin": {
      "tags": ["plugin"]
    },
    "comment-plugin": {
      "tags": ["plugin"]
    },
    "collaboration-plugin": {
      "tags": ["plugin"]
    }
  },
  "workspaceLayout": {
    "appsDir": "samples",
    "libsDir": "packages"
  }
}
